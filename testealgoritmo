Algoritmo "semnome"

Var

   temp: vetor[1..365] de inteiro
   i: inteiro
   temp_agr: inteiro
   media: real
   menor: real
   med: real
   v: inteiro
   maior: real
   o:inteiro


Inicio
   para i de 1 ate 365 faca
      leia(temp[i])
      med<- med + temp[i]
      maior<- temp[i]
      menor<- temp[i]
   fimpara
   media<- med / 365
   escreval(media)
   para i de 1 atï¿½ 365 faca
      se temp[i] < media entao
          v<- v+1
      fimse
      se maior > temp[i] entao
      maior<- temp[i]
      fimse
      se menor < temp[i] entao
      menor<- temp[i]
      fimse
      fimpara
      escreval(v)
      escreval(menor)
      escreval(maior)


Fimalgoritmo
